public class TwoDimensionalArray 
{
    public static void main(String[] args) 
    {
        Nums obj = new Nums();
        obj.init();
    }
}

class Nums
 {
    public void init() 
    {
        for(int i = 0; i < 10; i++) 
        {
            for(int j = 0; j < 10; j++)
             {
                System.out.print("(" + i + "," + j + ")\t");
            }
            System.out.println();
        }
    }

}
